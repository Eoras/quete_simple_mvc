{% extends '_layout.html.twig' %}

{% block title %}
    {{ parent() }} - Item list
{% endblock %}

{% block content %}
<h1>Items</h1>
    <ul class="list-group">
        {% for item in items %}
            <a class="list-group-item" href="/item/{{ item.id }}">
                {{ item.title }}
            </a>
        {% endfor %}
    </ul>
{% endblock %}